	#КонецОбласти
				
			КонецЦикла;
			
			Если ЭтотОбъект["ПриводитьКлючКСтроке" + ИдентификаторБазы] 
				Или ЭтотОбъект["ПриводитьКлючКВерхнемуРегистру" + ИдентификаторБазы] 
